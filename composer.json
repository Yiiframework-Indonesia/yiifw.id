{
    "name": "Yiiframework-Indonesia/yii2-id",
    "description": "Yii 2 CMS",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/Yiiframework-Indonesia/yii2-id/issues?state=open",
        "source": "https://github.com/Yiiframework-Indonesia/yii2-id"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": ">=2.0.10",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0",
        "yiisoft/yii2-authclient": "~2.1.0",
        "yiisoft/yii2-jui": "~2.0",
        "deesoft/yii2-console": "~1.0",
        "deesoft/yii2-tools": "~1.0",
        "mdmsoft/yii2-admin": "~2.0",
        "mdmsoft/yii2-upload-file": "~2.0",
        "deesoft/yii2-queue": "^1.0@dev",
        "rmrevin/yii2-fontawesome": "~2.17",
        "paulzi/yii2-nested-intervals" : "^1.0",
        "yiisoft/yii2-imagine": "^2.1",
        "kartik-v/yii2-grid": "@dev",
        "kartik-v/yii2-tabs-x": "*",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-datepicker": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "deesoft/yii2-gii": "~1.0",
        "yiisoft/yii2-faker": "~2.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
